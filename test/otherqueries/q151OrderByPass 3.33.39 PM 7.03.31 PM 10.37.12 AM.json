{
    "title": "test courses_dept with wildcards at front",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_instructor": "**"
                    }
                },
                {
                    "AND": [
                        {
                            "EQ": {
                                "courses_avg": 83
                            }
                        },{
                            "IS": {
                                "courses_dept": "cpsc"
                            }
                        }
                    ]
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_instructor",
                "courses_pass",
                "courses_avg"
            ],
            "ORDER": "courses_pass"
        }
    },
    "isQueryValid": true,
    "result": [
        {"courses_instructor":"doucet, arnaud","courses_pass":14,"courses_avg":83},
        {"courses_instructor":"","courses_pass":14,"courses_avg":83},
        {"courses_instructor":"evans, william","courses_pass":22,"courses_avg":83},
        {"courses_instructor":"","courses_pass":22,"courses_avg":83}
    ]
}
