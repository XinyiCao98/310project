{
    "title": "more in OR",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "OR": [
                        {
                            "GT": {
                                "courses_avg": 90
                            }
                        },
                        {
                            "EQ": {
                                "courses_pass": 90
                            }
                        }
                    ]
                },
                {
                    "GT": {
                        "courses_avg": 70
                    }
                },
                {
                    "GT": {
                        "courses_fail": 10
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_avg"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
