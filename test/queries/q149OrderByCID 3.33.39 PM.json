{
    "title": "test courses_dept with wildcards at front",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "cps*"
                    }
                },
                {
                    "LT": {
                        "courses_avg": 65
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_year",
                "courses_dept",
                "courses_id"
            ],
            "ORDER": "courses_id"
        }
    },
    "isQueryValid": true,
    "result":[
        {"courses_year":2012,"courses_dept":"cpsc","courses_id":"213"},
        {"courses_year":2009,"courses_dept":"cpsc","courses_id":"513"},
        {"courses_year":1900,"courses_dept":"cpsc","courses_id":"513"}
    ]
}
